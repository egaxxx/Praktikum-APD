    <function name="Main" type="None" variable="">
        <parameters/>
        <body>
            <declare name="j, h, hargbeli, hargdiskon, diskon, sisa, thd" type="Real" array="False" size=""/>
            <declare name="nim" type="Integer" array="False" size=""/>
            <declare name="n" type="String" array="False" size=""/>
            <assign variable="hargbeli" expression="0"/>
            <output expression="&quot;Apa yang ingin anda beli&quot;" newline="True"/>
            <input variable="n"/>
            <output expression="&quot;masukkan harga barang&quot;" newline="True"/>
            <input variable="h"/>
            <output expression="&quot;masukkan jumlah yang anda ingin beli&quot;" newline="True"/>
            <input variable="j"/>
            <output expression="&quot;Masukkan dua digit terakhir nim&quot;" newline="True"/>
            <input variable="nim"/>
            <assign variable="hargbeli" expression="j * h"/>
            <assign variable="diskon" expression="nim/100"/>
            <assign variable="hargdiskon" expression="hargbeli * diskon"/>
            <assign variable="thd" expression="hargbeli - hargdiskon"/>
            <assign variable="sisa" expression="diskon%3"/>
            <output expression="&quot;Anda membeli &quot;&amp;j&amp;&quot; &quot;&amp;n&amp;&quot; dengan harga satuan : Rp.&quot;&amp; h &amp;&quot;, Total harga sebelum diskon : Rp.&quot;&amp; hargbeli &amp;&quot;, total diskon : Rp.&quot;&amp;hargdiskon &amp;&quot;, Total yang harus dibayar : Rp.&quot;&amp; thd&amp; &quot;, sisanya &quot;&amp;sisa" newline="True"/>
            <output expression="&quot;Terima kasih telah berbelanja di Toko kami&quot;" newline="True"/>
        </body>
